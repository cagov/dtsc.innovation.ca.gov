        </div>
      </div>
    </div>

    <!--site-header with logo-->
    <div class="site-header">
      <!-- Note the `with-logo` class next to `container`. Add this when you're including a logo. -->
      <div class="container with-logo">
        <a href="https://dtsc.ca.gov" class="grid-logo" aria-label="DCC logo">
          <img style="max-height: 65px;" src="https://dtsc.ca.gov/wp-content/uploads/sites/31/2021/07/DTSC_weblogo2021.png">
        </a>
      </div>
    </div>
    <!--end site-header-->

    <div id="page-container" class="page-container-ds">
      <main id="body-content">

          {% block content %}
            {{ content | safe }}
          {% endblock %}

      </main>
    </div>


  </body>
</html>